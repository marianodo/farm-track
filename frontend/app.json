{
  "expo": {
    "name": "Measure Me",
    "slug": "farm-track",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/main-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/main-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bdmetrics.measureme",
      "runtimeVersion": "1.0.1"
    },
    "android": {
      "icon": "./assets/images/main-icon.png",
      "config": {
        "googleMaps": {
          "apiKey": "process.env.EXPO_PUBLIC_GOOGLE_PLACES_API_KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main-icon.png",
        "backgroundImage": "./assets/images/main-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.bdmetrics.measureme",
      "versionCode": 2,
      "runtimeVersion": "1.0.1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "683963ba-26c0-43fe-a449-8522761f6f88"
      }
    },
    "owner": "bdmetrics",
    "updates": {
      "url": "https://u.expo.dev/683963ba-26c0-43fe-a449-8522761f6f88"
    }
  }
}
