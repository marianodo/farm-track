{
  "expo": {
    "name": "Measure Me",
    "slug": "farm-track",
    "version": "1.0.12",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/images/splash-bd-metrics.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/icon.jpeg",
      "splash": {
        "image": "./assets/images/splash-bd-metrics.jpeg",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.bdmetrics.measureme",
      "runtimeVersion": "1.0.1"
    },
    "android": {
      "icon": "./assets/images/icon.jpeg",
      "splash": {
        "image": "./assets/images/splash-bd-metrics.jpeg",
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      },
      "config": {
        "googleMaps": {
          "apiKey": "process.env.EXPO_PUBLIC_GOOGLE_PLACES_API_KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.bdmetrics.measureme",
      "versionCode": 3,
      "runtimeVersion": "1.0.1",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "edgeToEdgeEnabled": true
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "android": {
            "splashScreenRound": false
          }
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "683963ba-26c0-43fe-a449-8522761f6f88"
      }
    },
    "owner": "bdmetrics",
    "updates": {
      "url": "https://u.expo.dev/683963ba-26c0-43fe-a449-8522761f6f88"
    }
  }
}
